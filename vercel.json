{
  "version": 2,
  "builds": [
    {
      "src": "./src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}
